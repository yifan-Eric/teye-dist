{
  "name": "big-data",
  "description": "",
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "start": "cross-env NODE_ENV=development  webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src/js",
    "test_yh": "cross-env NODE_ENV=test_yh webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "test_jf": "cross-env NODE_ENV=test_jf webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "test_sx": "cross-env NODE_ENV=test_sx webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "elec_start": "npm run build&&electron .",
    "elec_restart": "electron .",
    "packageLinux": "electron-packager . 'Teye' --platform=linux --arch=x64 --out=./elec_dist --asar --app-version=2.0.1",
    "packageWin": "electron-packager . 'Teye' --platform=win32 --arch=x64  --out=./elec_dist --asar --app-version=2.0.1",
    "packageDarwin": "electron-packager . 'Teye' --platform=darwin --arch=x64  --out=./elec_dist --asar --app-version=2.0.1"
  },
  "keywords": [
    "react",
    "spa",
    "big-data"
  ],
  "author": "Moriarty",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.13.2",
    "cropperjs": "^1.3.5",
    "intl": "^1.2.5",
    "nprogress": "0.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-intl": "^2.7.1",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "rxjs": "^6.4.0",
    "tf-react-cpn": "^2.2.1",
    "tf-utils": "^2.2.5"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression-webpack-plugin": "1.1.12",
